{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "semi": "off",
        "eqeqeq": "off",
        "guard-for-in": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "class-methods-use-this": "off",
        "no-param-reassign": "off"
    }
}